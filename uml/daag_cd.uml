@startuml

Interface VSTInstrument

namespace daag {
	Interface ComposeStrategy{
		-compose()	
	}
	Class Composer{
		-time
		+listen()
		+fromSeed(Seed)
		+toSeed(Seed)
	}
	Interface VSTAdapter
	Interface FX
	Interface Instrument
	Interface Seed
	
	Class Clock{
		+Clock(Composer)
		+tick(interval)
		+stop()
	}

	Class Musician{
		-play()
		+count()
	}

	ComposeStrategy ..o Composer
	Composer o.. Seed
	Composer *-- Musician
	Composer *-- Clock
	VSTAdapter .. Instrument
	VSTAdapter .. FX
	VSTAdapter .> .VSTInstrument
	Instrument <|.. Musician
	FX <|.. Musician
}
@enduml